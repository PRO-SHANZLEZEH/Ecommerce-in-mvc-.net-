
@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/LayoutUser.cshtml";
}
@model Clothes.Models.Account
<div class="breadcrumbs">
    <div class="container">
        <ol class="breadcrumb breadcrumb1 animated wow slideInLeft animated" data-wow-delay=".5s" style="visibility: visible; animation-delay: 0.5s; animation-name: slideInLeft;">
            <li><a href="index.html"><span class="glyphicon glyphicon-home" aria-hidden="true"></span>Home</a></li>
            <li class="active">Register</li>
        </ol>
    </div>
</div>
<div class="container">
    <div class="register">
        <h2>Register</h2>
 @using (Html.BeginForm("Register", "Account", FormMethod.Post))
 {

            <div class="col-md-6  register-top-grid">
                 
                <div class="mation">
                    <label>user Name</label>
                    @Html.TextBoxFor(model => model.username, new { @class = "contact_input" })

                    <label>full Name</label>
                    @Html.TextBoxFor(model => model.fullname, new { @class = "contact_input" })

                    <label>Email Address</label>
                    @Html.TextBoxFor(model => model.email, new { @class = "contact_input" })

                    <label>Password</label>
                    @Html.PasswordFor(model => model.password, new { @class = "contact_input" })
                    <label>website</label>
                    @Html.TextBoxFor(model => model.website, new { @class = "contact_input" })
                </div>
                  
                
           
            
           
            </div>
            <div class="clearfix"> </div>



        <div class="register-but">
            <form action="#" method="post">
                <input type="submit" value="submit">
                <div class="clearfix"> </div>
            </form>
        </div>
 }
    </div>
</div>

